Options +FollowSymlinks
RewriteEngine On
RewriteBase /app/plugin/messagIn/page/
RewriteRule ^allMessages/$  allMessages.php [L]
RewriteRule ^addMessage/$  addMessage.php [L]

RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule (.*) /app/lib/assets/404.php

ErrorDocument 403 /app/lib/assets/404.php
ErrorDocument 404 /app/lib/assets/404.php